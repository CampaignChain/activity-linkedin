{
    "name": "campaignchain/activity-linkedin",
    "description": "Post a news update on a LinkedIn stream.",
    "keywords": ["linkedin","news"],
    "type": "campaignchain-activity",
    "homepage": "http://www.campaignchain.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Sandro Groganz",
            "email": "sandro@campaignchain.com"
        }
    ],
    "require": {
        "campaignchain/core": "1.0.0@beta | 1.0.0@rc",
        "campaignchain/location-linkedin": "1.0.0@beta | 1.0.0@rc",
        "campaignchain/operation-linkedin": "1.0.0@beta | 1.0.0@rc",
        "campaignchain/hook-due": "1.0.0@alpha | 1.0.0@beta | 1.0.0@rc"
    },
    "autoload": {
        "psr-4": { "CampaignChain\\Activity\\LinkedInBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-1.0.0-beta.1": "1.0.0-dev"
        },
        "campaignchain": {
            "kernel": {
                "classes": [
                    "CampaignChain\\Activity\\LinkedInBundle\\CampaignChainActivityLinkedInBundle"
                ],
                "routing":
                {
                    "name": "campaignchain_activity_linked_in",
                    "resource": "@CampaignChainActivityLinkedInBundle/Resources/config/routing.yml",
                    "prefix": "/"
                }
            }
        }
    }
}
